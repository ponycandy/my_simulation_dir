<?xml version="1.0" encoding="UTF-8"?>
<parameters>
    <linear_solver type="string">
        <value>ma57</value>
    </linear_solver>
    <tol type="double">
        <value>0.1</value>
    </tol>
    <jacobian_approximation type="string">
        <value>exact</value>
    </jacobian_approximation>
<!--finite-difference-values: user-provided structure, values by finite differences-->
  <!--exact: user-provided derivatives-->
  <nlp_scaling_method type="string">
        <value>gradient-based</value>
    </nlp_scaling_method>
  <check_derivatives_for_naninf type="string">
        <value>yes</value>
    </check_derivatives_for_naninf>
    <print_level type="int">
        <value>5</value>
    </print_level>
    <acceptable_obj_change_tol type="double">
        <value>0.01</value>
    </acceptable_obj_change_tol>
    <hessian_approximation type="string">
        <value>exact</value>
    </hessian_approximation>
<!--limited-memory: Perform a limited-memory quasi-Newton approximation-->

</parameters>
